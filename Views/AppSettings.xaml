<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Terms.Views.AppSettings"
             Title="App Settings">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="ClearPreferences" Text="Clear Preferences" WidthRequest="150" HorizontalOptions="Center" VerticalOptions="Center" Clicked="ClearPreferences_Clicked"></Button>
            <Button x:Name="LoadSampleData" Text="Load Sample Data" WidthRequest="150" HorizontalOptions="Center" Clicked="LoadSampleData_Clicked"></Button>
            <Button x:Name="ClearSampleData" Text="Clear Sample Data" WidthRequest="150" HorizontalOptions="Center" Clicked="ClearSampleData_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>